{
  "name": "sourcenet-backend",
  "version": "1.0.0",
  "description": "SourceNet - Decentralized Data Marketplace Backend",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "indexer:dev": "ts-node-dev --respawn --transpile-only src/indexer/indexer.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@mysten/sui": "^1.9.0",
    "@mysten/walrus": "^0.3.0",
    "@prisma/client": "^5.4.0",
    "pg": "^8.11.0",
    "ioredis": "^5.3.0",
    "bullmq": "^4.10.0",
    "tweetnacl": "^1.0.3",
    "libsodium-wrappers": "^0.7.13",
    "@noble/hashes": "^1.3.1",
    "aws-sdk": "^2.1467.0",
    "multer": "^2.0.0",
    "zod": "^3.22.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.0.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "ws": "^8.14.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0",
    "dayjs": "^1.11.0",
    "lodash": "^4.17.21",
    "jsonwebtoken": "^9.0.2",
    "form-data": "^4.0.0",
    "p-retry": "^5.1.2",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.0",
    "@types/pg": "^8.10.0",
    "@types/ws": "^8.5.4",
    "@types/multer": "^1.4.7",
    "@types/lodash": "^4.14.195",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.4.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": ["dist"],
  "keywords": [
    "sui",
    "walrus",
    "data-marketplace",
    "decentralized",
    "sourcenet"
  ],
  "license": "MIT"
}
